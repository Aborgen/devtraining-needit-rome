        <name>Get what_needed default options</name>
        <order/>
        <script><![CDATA[function onLoad() {
    var neededSelect = "u_what_needed";
	g_scratchpad.neededOptions = extractOptionValues(neededSelect);
}

function extractOptionValues(selectName) {
	var select = g_form.getElement(selectName);
	var options = [];
	for (var i = 0; i < select.options.length; ++i) {
		var value = select.options[i].value;
		var label = g_form.getOption(selectName, value).text; 
		options.push({
			value: value,
			label: label,
		});
	}
	
	return options;
}]]></script>
